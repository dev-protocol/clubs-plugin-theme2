{
	"name": "@devprotocol/clubs-plugin-akiba",
	"version": "0.1.0",
	"type": "module",
	"description": "Akiba theme plugin for clubs.",
	"main": "dist/index.js",
	"exports": {
		".": {
			"default": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"types": "./dist/index.d.ts",
	"files": [
		"dist/*.d.ts",
		"dist/*.js",
		"!**/*.test.*",
		"src"
	],
	"keywords": [
		"clubs-plugin"
	],
	"scripts": {
		"test": "vitest",
		"preview": "astro --root .preview/preview.ssr dev",
		"preview:static": "astro --root .preview/preview.static dev",
		"build": "rollup -c",
		"prebuild": "rimraf dist",
		"lint": "yarn lint:eslint && yarn lint:format",
		"lint:eslint": "eslint . --fix",
		"lint:format": "prettier --write '**/*.{ts,js,tsx,jsx,astro,vue,svelte,json,md,yml}'",
		"prepack": "yarn test --run && yarn prebuild && yarn build",
		"prepare": "husky",
		"typecheck": "astro check && tsc --noEmit && svelte-check && vue-tsc --noEmit --p tsconfig.vue.json"
	},
	"author": "Dev Protocol",
	"license": "MPL-2.0",
	"dependencies": {
		"@devprotocol/dev-kit": "8.7.0",
		"@devprotocol/util-ts": "4.0.0",
		"@wagmi/core": "^1.4.1",
		"@web3modal/wagmi": "^5.0.0",
		"bignumber.js": "9.1.2",
		"fast-average-color": "^9.4.0",
		"ramda": "0.30.1",
		"sass": "1.83.0",
		"swiper": "^11.1.15"
	},
	"peerDependencies": {
		"@devprotocol/clubs-core": "^2.1.1 || ^3.0.0",
		"@devprotocol/clubs-plugin-passports": "^0.1.0-beta.35",
		"@devprotocol/clubs-plugin-payments": "^0.3.0",
		"ethers": "^6.0.0"
	},
	"devDependencies": {
		"@astrojs/check": "^0.9.0",
		"@astrojs/svelte": "^6.0.0",
		"@astrojs/tailwind": "^5.0.0",
		"@astrojs/vue": "^4.0.0",
		"@devprotocol/clubs-core": "3.22.5",
		"@devprotocol/clubs-plugin-passports": "^0.1.0-beta.35",
		"@devprotocol/clubs-plugin-payments": "^0.3.0",
		"@eslint/js": "^9.17.0",
		"@rollup/plugin-commonjs": "^28.0.1",
		"@rollup/plugin-typescript": "12.1.2",
		"@tailwindcss/typography": "^0.5.10",
		"@types/dotenv": "8.2.3",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/eslint__js": "^8.42.3",
		"@types/node": "22.10.2",
		"@types/ramda": "0.30.2",
		"astro": "^4.0.0",
		"dotenv": "16.4.7",
		"eslint": "^9.17.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-functional": "^7.2.0",
		"ethers": "6.13.4",
		"globals": "^15.14.0",
		"husky": "9.1.7",
		"prettier": "3.4.2",
		"prettier-plugin-astro": "^0.14.0",
		"prettier-plugin-svelte": "^3.0.0",
		"prettier-plugin-tailwindcss": "^0.6.0",
		"rimraf": "6.0.1",
		"rollup": "4.28.1",
		"rollup-plugin-dts": "^6.1.1",
		"rollup-plugin-vue": "^6.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwindcss": "3.4.17",
		"typescript": "5.6.3",
		"typescript-eslint": "^8.18.1",
		"vitest": "^2.0.0",
		"vue": "3.5.13",
		"vue-tsc": "^2.0.0"
	},
	"packageManager": "yarn@4.5.3"
}
